{
  "name": "skatrych/vertica-php-adapter",
  "description": "PHP Adapter for Vertica Database from HP. Uses php ODBC extension.",
  "authors": [
    {
      "name": "Sergii Katrych",
      "email": "sergii.katrych@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*",
    "phpunit/dbunit": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "VerticaPhpAdapter\\" : "src/",
      "VerticaPhpAdapter\\Test\\" : "Tests/"
    }
  },
  "minimum-stability": "dev"
}
